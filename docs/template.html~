<section id="outline-container-orgc0fb8cb" class="outline-2">
<h2 id="orgc0fb8cb">Below is the content of <code>.gitlab-ci.yml</code>:</h2>
<div class="outline-text-2" id="text-orgc0fb8cb">
<div class="org-src-container">
<pre class="src src-conf">image: iquiw/alpine-emacs

.build: &amp;build
  script:
  - emacs --batch --no-init-file --load publish.el --funcall org-publish-all
  artifacts:
    paths:
    - public

pages:
  &lt;&lt;: *build
  only:
  - master

test:
  &lt;&lt;: *build
  except:
  - master
</pre>
</div>
</div>
</section>

