<section id="outline-container-orgd35ddf3" class="outline-2">
<h2 id="orgd35ddf3">Below is the content of <code>.gitlab-ci.yml</code>:</h2>
<div class="outline-text-2" id="text-orgd35ddf3">
<div class="org-src-container">
<pre class="src src-conf">image: iquiw/alpine-emacs

.build: &amp;build
  script:
  - emacs --batch --no-init-file --load publish.el --funcall org-publish-all
  artifacts:
    paths:
    - public

pages:
  &lt;&lt;: *build
  only:
  - master

test:
  &lt;&lt;: *build
  except:
  - master
</pre>
</div>
</div>
</section>

